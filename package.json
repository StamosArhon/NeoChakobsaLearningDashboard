{
  "name": "neochakobsa-dashboard",
  "private": true,
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "scripts": {
    "setup:backend": "python -m pip install -r backend/requirements.txt",
    "setup:frontend": "npm --prefix frontend install --no-save --no-package-lock",
    "setup": "npm run setup:backend && npm run setup:frontend",
    "dev": "concurrently \"uvicorn backend.server:app --reload\" \"npm --prefix frontend run dev\""
  }
}
